<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title></title>


</head>

<textarea id="language" rows="10" cols="50" style="display: none">
    {{{sequenceDiagram}}}
</textarea>

<div style="font-size:80%; text-align:center; display: inline-block">
    <div id="diagram" style="padding-bottom:0.5em;"></div>
</div>

<link href="bower_components/js-sequence-diagrams/dist/sequence-diagram-min.css" rel="stylesheet" />
<script src="bower_components/jquery/dist/jquery.min.js"></script>
<script src="bower_components/bower-webfontloader/webfont.js"></script>
<script src="bower_components/snap.svg/dist/snap.svg-min.js"></script>
<script src="bower_components/underscore/underscore-min.js"></script>
<script src="bower_components/js-sequence-diagrams/dist/sequence-diagram-min.js"></script>

<script>
    $(document).ready(function () {
        var diagram = Diagram.parse($('#language').val());
        diagram.drawSVG('diagram', {theme: '{{theme}}'});
    });
</script>
</body>
</html>